{
  "name": "@linonetwo012/zhihu-nlp-playground",
  "version": "1.0.0",
  "description": "Do some annotation on zhihu dataset.",
  "main": "index.js",
  "scripts": {
    "run": "npm run build && MKCERT_PATH=\"$(mkcert -CAROOT)/rootCA.pem\" node ./dist/index.js",
    "baas-start": "mkdir -p ./solid && cd ./solid && solid start --root $(pwd)/data --data-browser-path $(pwd)/../node_modules/mashlib/dist/databrowser.html --config-file $(pwd)/../config.solid.example.json",
    "generate-keys": "mkdir -p ./keys && cd ./keys && mkcert localhost zhihucrawler.localhost 127.0.0.1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linonetwo/zhihu-nlp-playground.git"
  },
  "keywords": [
    "crawler",
    "zhihu",
    "nlp"
  ],
  "author": "linonetwo",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/linonetwo/zhihu-nlp-playground/issues"
  },
  "homepage": "https://github.com/linonetwo/zhihu-nlp-playground#readme",
  "dependencies": {
    "@solid/query-ldflex": "^2.8.1",
    "@types/node-fetch": "^2.5.4",
    "node-fetch": "^2.6.0",
    "solid-auth-cli": "^1.0.10",
    "solid-server": "^5.2.2",
    "ssl-root-cas": "^1.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "typescript": "^3.7.4"
  }
}
