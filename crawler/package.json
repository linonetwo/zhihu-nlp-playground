{
  "name": "@linonetwo012/zhihu-nlp-playground",
  "version": "1.0.0",
  "description": "Do some annotation on zhihu dataset.",
  "main": "index.js",
  "scripts": {
    "run:qa": "npm run build && node ./dist/questionsAnswersCrawler/index.js",
    "db:qa:create": "npm run build && node ./dist/questionsAnswersCrawler/scripts/createDB.js",
    "db:qa:addTask": "npm run build && node ./dist/questionsAnswersCrawler/scripts/addTasks.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "rescripts": [
    [
      "use-babel-config",
      ".babelrc.js"
    ],
    "disable-eslint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linonetwo/zhihu-nlp-playground.git"
  },
  "keywords": [
    "crawler",
    "zhihu",
    "nlp"
  ],
  "author": "linonetwo",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/linonetwo/zhihu-nlp-playground/issues"
  },
  "homepage": "https://github.com/linonetwo/zhihu-nlp-playground#readme",
  "dependencies": {
    "@types/node": "^13.1.6",
    "@zeit/fetch-retry": "^4.1.0",
    "bluebird": "^3.7.2",
    "knex": "^0.20.7",
    "node-fetch": "^2.6.0",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-async-generator-functions": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@rescripts/cli": "0.0.13",
    "@rescripts/rescript-env": "0.0.11",
    "typescript": "^3.7.4"
  }
}
